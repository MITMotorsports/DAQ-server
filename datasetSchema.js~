var mongoose = require('mongoose');
var Schema = mongoose.Schema;

var datasetSchema = new Schema({
    name: String,
    uploadDate: Date,
    data: [
	{
	    key: String,
	    name: String,
	    units: String,
	    values: [
		{
		    time: Number,
		    value: Schema.Types.Mixed
		}
	    ]
	}
    ]
});
